{
  "name": "@iyulab/u-components",
  "description": "iyulab ui components",
  "version": "0.0.62",
  "author": {
    "name": "iyulab",
    "url": "https://www.iyulab.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "test": "vite --force",
    "scan": "i18next -c i18next.config.js",
    "demo": "storybook dev -p 6006",
    "build": "iyulab build -d dist -b vite",
    "build:demo": "storybook build -o docs",
    "deploy": "iyulab publish"
  },
  "exports": {
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.js",
      "types": "./src/index.d.ts"
    },
    "./*": {
      "require": "./src/*/index.js",
      "import": "./src/*/index.js",
      "types": "./src/*/index.d.ts"
    },
    "./styles/*": {
      "import": "./static/styles/*.css"
    },
    "./decorators": {
      "require": "./src/decorators/index.js",
      "import": "./src/decorators/index.js",
      "types": "./src/decorators/index.d.ts"
    },
    "./localization": {
      "require": "./src/localization/index.js",
      "import": "./src/localization/index.js",
      "types": "./src/localization/index.d.ts"
    },
    "./utils": {
      "require": "./src/utils/index.js",
      "import": "./src/utils/index.js",
      "types": "./src/utils/index.d.ts"
    },
    "./components/*": {
      "require": "./src/components/*/index.js",
      "import": "./src/components/*/index.js",
      "types": "./src/components/*/index.d.ts"
    },
    "./components/modal/*": {
      "require": "./src/components/modal/*.js",
      "import": "./src/components/modal/*.js",
      "types": "./src/components/modal/*.d.ts"
    },
    "./components/button/*": {
      "require": "./src/components/button/*.js",
      "import": "./src/components/button/*.js",
      "types": "./src/components/button/*.d.ts"
    },
    "./components/input/*": {
      "require": "./src/components/input/*.js",
      "import": "./src/components/input/*.js",
      "types": "./src/components/input/*.d.ts"
    },
    "./components/input-parts/*": {
      "require": "./src/components/input-parts/*.js",
      "import": "./src/components/input-parts/*.js",
      "types": "./src/components/input-parts/*.d.ts"
    },
    "./components/input-extension/*": {
      "require": "./src/components/input-extension/*.js",
      "import": "./src/components/input-extension/*.js",
      "types": "./src/components/input-extension/*.d.ts"
    },
    "./components/input-advanced/*": {
      "require": "./src/components/input-advanced/*.js",
      "import": "./src/components/input-advanced/*.js",
      "types": "./src/components/input-advanced/*.d.ts"
    }
  },
  "dependencies": {
    "@lit/react": "^1.0.5",
    "lit": "^3.1.3",
    "mobx": "^6.12.3",
    "react": "^18.3.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@iyulab/utils": "^0.0.8",
    "@shoelace-style/shoelace": "^2.15.0",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/web-components": "^8.0.9",
    "@storybook/web-components-vite": "^8.0.9",
    "@types/node": "^20.12.7",
    "i18next": "^23.11.2",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.5.1",
    "i18next-parser": "^8.13.0",
    "lit-i18n": "^4.1.0",
    "monaco-editor": "^0.48.0",
    "postcss": "^8.4.38",
    "postcss-nesting": "^12.1.2",
    "react-i18next": "^14.1.1",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "storybook": "^8.0.9",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-dts": "^3.9.0"
  }
}
