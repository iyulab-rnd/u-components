{
  "main": "index.cjs.js",
  "module": "index.es.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./index.cjs.js",
      "import": "./index.es.js",
      "types": "./index.d.ts"
    },
    "./*": {
      "require": "./*/index.cjs.js",
      "import": "./*/index.es.js",
      "types": "./*/index.d.ts"
    },
    "./styles/*": {
      "import": "./styles/*.css"
    },
    "./decorators": {
      "require": "./decorators/index.js",
      "import": "./decorators/index.js",
      "types": "./decorators/index.d.ts"
    },
    "./localization": {
      "require": "./localization/index.js",
      "import": "./localization/index.js",
      "types": "./localization/index.d.ts"
    },
    "./utils": {
      "require": "./utils/index.js",
      "import": "./utils/index.js",
      "types": "./utils/index.d.ts"
    },
    "./components/*": {
      "require": "./components/*/index.cjs.js",
      "import": "./components/*/index.es.js",
      "types": "./components/*/index.d.ts"
    },
    "./components/modal/*": {
      "require": "./components/modal/*.js",
      "import": "./components/modal/*.js",
      "types": "./components/modal/*.d.ts"
    },
    "./components/button/*": {
      "require": "./components/button/*.cjs.js",
      "import": "./components/button/*.es.js",
      "types": "./components/button/*.d.ts"
    },
    "./components/input/*": {
      "require": "./components/input/*.cjs.js",
      "import": "./components/input/*.es.js",
      "types": "./components/input/*.d.ts"
    },
    "./components/input-parts/*": {
      "require": "./components/input-parts/*.cjs.js",
      "import": "./components/input-parts/*.es.js",
      "types": "./components/input-parts/*.d.ts"
    },
    "./components/input-extension/*": {
      "require": "./components/input-extension/*.cjs.js",
      "import": "./components/input-extension/*.es.js",
      "types": "./components/input-extension/*.d.ts"
    },
    "./components/input-advanced/*": {
      "require": "./components/input-advanced/*.cjs.js",
      "import": "./components/input-advanced/*.es.js",
      "types": "./components/input-advanced/*.d.ts"
    }
  }
}